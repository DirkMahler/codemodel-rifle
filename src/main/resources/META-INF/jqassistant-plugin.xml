<jqa-plugin:jqassistant-plugin
        xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.1" name="JavaScript">
    <description>Provides a scanner for CSV file.</description>
    <model>
        <class>hu.bme.mit.codemodel.rifle.jqassistant.model.JavaScriptDescriptor</class>
        <class>hu.bme.mit.codemodel.rifle.jqassistant.model.JavaScriptFileDescriptor</class>
    </model>
    <scanner>
        <class>hu.bme.mit.codemodel.rifle.jqassistant.scanner.JavaScriptFileScannerPlugin</class>
    </scanner>
</jqa-plugin:jqassistant-plugin>